#include <iostream>
using namespace std;

int main() {
    int n = 121, original, reversed = 0, rem;
    original = n;

    while (n > 0) {
        rem = n % 10;
        reversed = (reversed * 10) + rem;
        n /= 10;
    }

    if (original == reversed)
        cout << original << " is a Palindrome.";
    else
        cout << original << " is not.";

    return 0;
}
